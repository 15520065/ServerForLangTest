{
  "name": "argon-dashboard-react",
  "version": "1.0.0",
  "description": "React version of Code Gen by UIT",
  "main": "index.js",
  "keywords": [
    "react",
    "reactjs"
  ],
  "author": "UIT",
  "license": "MIT",
  "homepage": "https://qrcode-free.com",
  "bugs": {
    "url": "https://github.com/creativetimofficial/argon-dashboard-react/issues"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "SET NODE_ENV=production & lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{js,jsx},!*.min.js": [
      "pretty-quick --staged",
      "eslint src/ --fix",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "nouislider": "^13.1.5",
    "promise.prototype.finally": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-app-rewire-styled-components": "^3.0.2",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.8.6",
    "customize-cra": "^0.2.12",
    "react-loading-screen": "^0.0.17",

    "react-router-dom": "^5.0.0",
    "react-router-sitemap": "^1.2.0",
    "react-scripts": "^3.0.0",
    "reactstrap": "^8.0.0",
    "styled-components": "^4.2.0",
    "superagent": "^5.0.5",
    "superagent-promise": "^1.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@types/markerclustererplus": "^2.1.33",
    "@types/react": "^16.8.16",
    "browserslist": "^4.5.5",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0i",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.13.0",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0",
    "typescript": "^3.4.5"
  }
}
